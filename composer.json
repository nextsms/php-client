{
    "name": "nextsms/php-client",
    "description": "Nextsms for PHP",
    "keywords": [
        "nextsms",
        "php-client"
    ],
    "homepage": "https://github.com/nextsms/php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpha Olomi",
            "email": "alphaolomi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nextsms\\Nextsms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nextsms\\Nextsms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
